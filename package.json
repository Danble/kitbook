{
  "name": "svelte-pieces",
  "version": "1.0.14",
  "description": "Useful Svelte UI pieces for building web apps",
  "type": "module",
  "scripts": {
    "dev": "svelte-kit dev -p 3210",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json --threshold warning --diagnostic-sources js,svelte",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --threshold warning --diagnostic-sources js,svelte --watch",
    "lint": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "package": "svelte-kit package",
    "release": "npm run package && cd package && npm publish",
    "postversion": "git push && git push --tags && npm run release",
    "test": "vitest -u",
    "check-packages": "pnpm update --latest --interactive"
  },
  "dependencies": {
    "fast-xml-parser": "^3.19.0",
    "he": "^1.2.0",
    "recordrtc": "^5.6.2",
    "yootils": "^0.3.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.22.0",
    "@sveltejs/adapter-auto": "1.0.0-next.40",
    "@sveltejs/kit": "1.0.0-next.323",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.41",
    "@testing-library/jest-dom": "^5.16.2",
    "@types/recordrtc": "^5.6.8",
    "@types/youtube": "^0.0.46",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-svelte3": "^3.4.0",
    "mdsvex": "^0.10.5",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.47.0",
    "svelte-check": "^2.7.0",
    "svelte-deep-wind-preprocess": "^0.0.7",
    "svelte-preprocess": "^4.10.6",
    "svelte-store-router": "^2.0.10",
    "svelte-windicss-preprocess": "~4.1.0",
    "svelte2tsx": "^0.5.9",
    "tslib": "^2.4.0",
    "typescript": "^4.0.0",
    "vitest": "^0.10.0",
    "windicss": "^3.5.1"
  },
  "peerDependencies": {
    "svelte": "^3.46.4",
    "typescript": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacob-8/svelte-pieces.git"
  },
  "keywords": [
    "Svelte",
    "SvelteKit",
    "Typescript",
    "UI"
  ],
  "author": "Jacob Bowdoin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jacob-8/svelte-pieces/issues"
  },
  "homepage": "https://github.com/jacob-8/svelte-pieces#readme"
}
